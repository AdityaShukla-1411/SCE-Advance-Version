{
  "name": "sce-advanced",
  "version": "1.0.0",
  "description": "Professional Code Evaluator for Universities - Advanced plagiarism detection and bulk processing",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:backend": "cd backend && pnpm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "build:backend": "cd backend && pnpm run build",
    "build:frontend": "cd frontend && pnpm run build",
    "start": "concurrently \"pnpm run start:backend\" \"pnpm run start:frontend\"",
    "start:backend": "cd backend && pnpm start",
    "start:frontend": "cd frontend && pnpm start",
    "install:all": "pnpm install && cd backend && pnpm install && cd ../frontend && pnpm install",
    "test": "pnpm run test:backend && pnpm run test:frontend",
    "test:backend": "cd backend && pnpm test",
    "test:frontend": "cd frontend && pnpm test",
    "lint": "pnpm run lint:backend && pnpm run lint:frontend",
    "lint:backend": "cd backend && pnpm run lint",
    "lint:frontend": "cd frontend && pnpm run lint",
    "clean": "rimraf node_modules backend/node_modules frontend/node_modules backend/.next frontend/.next",
    "setup": "pnpm run install:all && pnpm run setup:env",
    "setup:env": "cp backend/.env.example backend/.env"
  },
  "keywords": [
    "code-evaluator",
    "plagiarism-detection",
    "education",
    "university",
    "code-analysis",
    "bulk-processing",
    "academic",
    "grading",
    "assignment-evaluation"
  ],
  "author": "SCE Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/sce-advanced.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/sce-advanced/issues"
  },
  "homepage": "https://github.com/your-username/sce-advanced#readme"
}
